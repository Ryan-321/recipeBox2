<div class="index-wrapper">
  {{#if recipes}}
    <div class="container-fluid">
        {{#each recipes}}
            <div class="recipe-card">
                <a href="/user/{{userId}}/recipes/{{id}}"><img src="{{photo_url}}" alt="" /></a>
            </div>
        {{/each}}
    </div>
  {{else}}
    <div class="container-fluid">
      <h1>You need to get some recipes.  Start Creating!!!!!!!</h1>
    </div>
  {{/if}}

    {{#if message}}
        <h1>{{message}}</h1>
    {{/if}}
</div>
